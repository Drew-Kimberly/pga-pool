{
  "name": "pga-pool-api",
  "version": "0.0.1",
  "description": "",
  "author": "Drew Kimberly",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "test": "vitest run --passWithNoTests",
    "typeorm": "ts-node -r tsconfig-paths/register tools/typeorm.ts",
    "pgapool": "ts-node cli/index.ts",
    "cron:tournament-score-updater": "ts-node src/cron/jobs/tournament-score-updater.ts",
    "db:migrations:generate": "yarn typeorm migration:generate --pretty",
    "db:migrations:run": "yarn typeorm migration:run",
    "db:migrations:revert": "yarn typeorm migration:revert",
    "db:drop": "yarn typeorm schema:drop",
    "sdk:build": "yarn --cwd ../client/sdk compile",
    "sdk:generate": "yarn openapi-generator-cli generate --generator-key pga-pool-api --remove-operation-id-prefix",
    "sdk:generate:build": "yarn sdk:generate && yarn sdk:build",
    "async-worker": "ts-node src/async-worker/index.ts",
    "start:async-worker": "cross-env ASYNC_WORKER_JITTER_MS=10000 ts-node src/async-worker/index.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "11.1.14",
