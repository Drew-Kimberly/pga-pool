    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.2.0",
    "node-html-parser": "6.1.12",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.11.3",
    "reflect-metadata": "0.2.1",
