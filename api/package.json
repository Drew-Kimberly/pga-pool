    "graphql": "16.13.0",
    "graphql-request": "7.4.0",
    "graphql-tag": "2.12.6",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.3",
    "jwks-rsa": "3.2.2",
    "node-html-parser": "7.0.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.19.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "typeorm": "0.3.28"
  },
